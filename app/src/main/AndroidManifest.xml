<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lock0134.mobileguifinalproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Quiz Maker"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        >
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".QuizStart">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MovieInformation" />
        <activity android:name=".MovieDetails"></activity>

        <activity android:name=".OCTranspo" />
        <activity android:name=".TranspoDetails"></activity>

        <activity android:name=".QuestionDetails" />
        <activity android:name=".QuestionStats" />

    </application>

</manifest>